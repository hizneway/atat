{
  "name": "atat",
  "version": "1.0.0",
  "description": "ATAT Stateless Services",
  "main": "index.js",
  "scripts": {
    "watch": "parcel watch js/index.js -d static/assets --public-url /static/assets -o index.js --no-autoinstall",
    "build": "parcel build js/index.js -d static/assets --public-url /static/assets -o index.js",
    "build-prod": "parcel build js/index.js -d static/assets --public-url $CDN_URL -o index.js",
    "test": "jest",
    "test:coverage": "jest --coverage --collectCoverageFrom='js/**/*.js'",
    "test:watch": "jest --watch --no-cache"
  },
  "author": "",
  "license": "MIT",
  "dependencies": {
    "@azure/storage-blob": "^12.2.1",
    "ally.js": "^1.4.1",
    "autoprefixer": "^9.8.6",
    "babel-polyfill": "^6.26.0",
    "date-fns": "^2.16.1",
    "ramda": "^0.27.1",
    "stickybits": "^3.7.7",
    "svg-innerhtml": "^1.1.0",
    "text-mask-addons": "^3.8.0",
    "uswds": "^1.6.13",
    "v-tooltip": "^2.0.3",
    "vue": "2.6.12",
    "vue-text-mask": "^6.1.2",
    "whatwg-fetch": "^3.4.1"
  },
  "devDependencies": {
    "@vue/test-utils": "^1.1.0",
    "babel-core": "^6.26.3",
    "babel-jest": "^23.6.0",
    "babel-preset-env": "^1.7.0",
    "jest": "^26.4.2",
    "jest-serializer-vue": "^2.0.2",
    "node-sass": "^4.14.1",
    "parcel-bundler": "^1.10.3",
    "prettier": "^2.1.2",
    "vue-template-compiler": "2.6.12"
  },
  "resolutions": {
    "node-forge": "^0.10.0",
    "parcel-bundler/**/lodash": "^4.17.13",
    "minimist": ">=1.2.2",
    "acorn": ">=5.7.4 <6.0.0 || >=6.4.1 <7.0.0 || >=7.1.1",
    "uswds/**/acorn": "^6.4.1",
    "kind-of": ">=6.0.3"
  },
  "browserslist": [
    "last 3 version",
    "> 5%",
    "IE 10"
  ],
  "jest": {
    "transform": {
      "^.+\\.js$": "<rootDir>/node_modules/babel-jest"
    },
    "snapshotSerializers": [
      "<rootDir>/node_modules/jest-serializer-vue"
    ],
    "setupFiles": ["<rootDir>/js/test_utils/setup"]
  }
}
